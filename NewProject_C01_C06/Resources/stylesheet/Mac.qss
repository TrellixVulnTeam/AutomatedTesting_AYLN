/**********QToolBar ***********/

QToolBar
{
        background-color: transparent;
        font:14pt "Arial";
        min-height: 1.5em;
        max-height: 1.5em;
}


QDockWidget
{
    background-color: rgb(41, 57, 86);
}

QToolTip {
    color: yellow;
    background-color: rgb(45, 45, 45);
    border: 1px solid rgb(100, 100, 100);
}



QTabBar::tab
{
    border-bottom-color: #C2C7CB;
    margin:0.1em;
    max-width: 10em; min-width:4em; min-height:1.6em;
    padding: 0px;
}

QTabBar::tab:first:selected
{
    color: black;
   background-color: rgb(255, 242, 157);
}

QTabBar::tab:selected
{
    color: black;
   background-color: rgb(255, 242, 157);
}

QTabBar::tab:first:!selected
{
    color: white;
    background-color: #C2C7CB;
}

QTabBar::tab:first:hover:!selected
{
    color: white;
    background-color: rgb(91, 113, 153);
}

QTabBar::tab:middle:selected
{
    color: black;
   background-color: rgb(255, 242, 157);
}

QTabBar::tab:middle:!selected
{
    color: white;
    background-color: #C2C7CB;
}

QTabBar::tab:middle:hover:!selected
{
    color: white;
     background-color: rgb(91, 113, 153);
}

QTabBar::tab:last:selected
{
    color: black;
   background-color: rgb(255, 242, 157);
}

QTabBar::tab:last:!selected
{
    color: white;
    border-image: url();
    background-color: #C2C7CB;
}

QTabBar::tab:last:hover:!selected
{
    color: white;
    border-image: url();
    background-color: rgb(91, 113, 153);
}

QTabBar::tab:only-one
{
    margin: 0;
}

QTabBar::scroller
{
    width:25;
    border:0;
    padding: 0px;
}


